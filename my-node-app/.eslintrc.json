{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "error",
    "no-console": "off",
    "semi": "error",
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "no-var": "error",
    "prefer-const": "error"
  }
}
